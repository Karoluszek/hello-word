age=int(input('Ile masz lat?'))
responsible = input('Czy jesteś odpowiedzialny? T/N') 

if age>100:
    print('200 lat!♫')
elif age>=18 and responsible=='T':
    print('Jesteś dorosły')
elif age>=18 and responible!="T":
    print('popracuj nad odpowiedzialnoscią')
else:
    print('Jeszcze troche ci brakuje do dorosłosci')
