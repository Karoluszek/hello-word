DNA=DNA.upper()
DNA=DNA.replace(" ","")
print(DNA,'\n')
A=DNA.count('A')
C=DNA.count ('C')
T=DNA.count ('T')
G=DNA.count ('G')
print('adenina: {}, cytozyna: {}, guanina: {}, tymina: {}'.format(A,C,G,T))
GAGA=DNA.count('GAGA')
print('Liczba wyst GAGA', GAGA)
DNA=DNA.replace('GAGA','AGAG')
CTGAAA=DNA.find('CTGAAA')
print('Liczba wyst CTGAAA', CTGAAA)
CTGAA=DNA.find('CTGAA-')
print('Liczba wyst CTGAA*', CTGAA)
print('Liczba wystąpień CTGAAA i CTGAA', CTGAA + CTGAAA)
DNA=DNA.replace("-","")
RNA=DNA.replace('T','U')
print(RNA)
